#include <fcntl.h>
#include <stdlib.h>
#include "map.h"

t_map		*load_map(char *file)
{
	t_map	*map;
	int		fd;

	if ((fd = open(file, O_RDONLY)) == -1)
		return (NULL);
	return (map);
}
